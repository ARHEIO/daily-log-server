    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^2.3.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.2",
    "wait-on": "^5.0.0",
    "winston": "^3.2.1"
  }
